<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet"
              href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}">
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<body>
<div class="container-fluid">
    {% block nav %}
        <ul class="nav nav-tabs nav-justified">
            <li><a href="{{ path('name_rank_person_new') }}">Add Person</a></li>
            <li><a href="{{ path('name_rank_new') }}">Add Names</a></li>
            <li><a href="{{ path('name_rank_compare_as') }}">Compare names</a></li>
            <li><a href="{{ path('name_rank_names') }}">Show Rank</a></li>
            <li><a href="{{ path('name_rank_name_updateall') }}">Update All</a></li>

        </ul>
    {% endblock %}
    {% block body %}{% endblock %}
</div>
{% block javascripts %}
    <script href="{{ asset('assets/vendor/jquery/dist/jquery.min.js') }}"></script>
    <script href="{{ asset('assets/vendor/bootstrap/dist/js/bootstrap.min.js') }}"></script>

{% endblock %}
</body>
</html>
